{
  "name": "hapi-sentry",
  "version": "4.0.0",
  "description": "hapi plugin for @sentry/node",
  "main": "index.js",
  "scripts": {
    "test": "ava -v test.mjs --serial",
    "lint": "eslint --ignore-path .gitignore .",
    "lint-fix": "eslint --fix --ignore-path .gitignore ."
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "arrow-parens": 0,
      "strict": 0,
      "no-underscore-dangle": 0,
      "no-param-reassign": 0
    },
    "plugins": [
      "ava"
    ]
  },
  "repository": "github:hydra-newmedia/hapi-sentry",
  "keywords": [
    "hapi",
    "sentry",
    "plugin"
  ],
  "author": "Christian Hotz <hotz@hydra-newmedia.com>",
  "license": "MIT",
  "peerDependencies": {
    "@hapi/hapi": ">=19 <22"
  },
  "dependencies": {
    "@hapi/hoek": "^11.0.2",
    "@sentry/node": "^7.38.0",
    "@sentry/types": "^7.38.0",
    "joi": "^17.7.1"
  },
  "devDependencies": {
    "@hapi/hapi": "^21.3.0",
    "ava": "^5.2.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-import": "^2.27.5",
    "p-defer": "^4.0.0"
  }
}
